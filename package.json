{
    "name": "safe-sftp-upload",
    "displayName": "Safe SFTP Upload",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.70.0"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
      "commands": [
        {
